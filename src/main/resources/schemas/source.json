{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"someSourceString": {
			"type": "string"
		},
		"someSourceInteger": {
			"type": "integer"
		},
		"someSourceStringArray": {
			"type": "array",
			"items": {
				"type": "array",
				"items": {
					"type": "string"
				}
			}
		},
		"someSourceIntegerArray": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		}
	},
	"additionalProperties" : false,
	"required": ["someSourceString", "someSourceInteger", "someSourceStringArray", "someSourceIntegerArray"]
}
